        Long enteroGrandeObejct;
        System.out.println("el rango de un long es : negativo 9,223,372,036,854,775,808 a 9,223,372,036,854,775,807");
        short enteroPequeno;
        Short enteroPequenoObject;
         System.out.println("El rando de un short es: 32,768 negativo a 127 positivo.");
         
         byte enteroMuyPequeno;
         Byte enteroMuyPequenoObject; 
         System.out.println("El rango de byte es: 128 negativo a 127 positivo.");
        
        //boolean
        boolean bool;
        Boolean booleanObject;
        System.out.println(" El boolean solo es de verdadero o falso.");
        
        //Decimal
        float decimal;
        Float decimalObject;
         System.out.println("el rango de un float es: 1.4e negativo 045 to 3.4e Positivo 038");
        
        double decimal2;
        Double decimal2Object;
        System.out.println (" El rango de un double:4.9e Negativo 324 to 1.8e Positivo 308");
        

        // cadena de caracteres
        String cadena;
        System.out.println("el string puede imprimir cadenas de palabras.");
        
        //caracter
        char caracter;
         System.out.println("el char puede tener un simbolo, una letra.o un valor numerico que represente una letra ");
        
